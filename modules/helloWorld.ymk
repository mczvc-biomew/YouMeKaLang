fun hello() {
    var hi = "Hello";
    var welcome = "welcome";
    puts hi;
    return {
        world: " World!",
        greetings: || hi + this.world,
        greetings2: || "Hello! Greetings and " + welcome,
        greetings3: || this.greetings2() + "!"
    };
}

interface Greeter {
    greet(msg: string);
}

class ConcreteGreeter > Greeter {}
class CompleteGreeter > Greeter {
    greet(msg: string) {
        print msg;
    }
}
