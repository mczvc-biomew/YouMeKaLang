fun hello() {
    return {
        world: "Hello World!"
    };
}
hello();
print hello();
print hello()["world"];

// lambda
var add = |a,b| a+b;
var triple = |x| x*3;
var always = || 42;
var HelloWorld = || { return "Hello || World!"; };

print add(1,2);
print triple(3);
print triple(4);
print always();
print HelloWorld();

var noReturn = || {};
print noReturn() == undefined;
print noReturn;